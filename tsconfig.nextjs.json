{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"plugins": [
			{
				"name": "next"
			}
		]
	},
	"include": [
		"next-env.d.ts",
		"middleware.ts",
		".next/types/**/*.ts",
		"app/**/*.ts",
		"app/**/*.tsx"
	],
	"exclude": [
		"node_modules",
		"**/__fixtures__/**",
		"**/__mocks__/**",
		"**/__specs__/**"
	]
}
